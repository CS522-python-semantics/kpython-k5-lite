// Testing new, constructor, method invocation.

class C {
  i = 0;
  j = 0;
  def C(x) {
    i = x;
    j = ++x;
  }
  def add(d) {
    i = i+d;
    j = j-d;
  }
  def print2() {
    print(i, " ", j, "\n");
  }
}

class Main {
  a = 0;
  b = 0;
  o = 0;
  def Main(){
    a = b = 5;
    o = new C(a);
    o.print2();
    o.add(++b);
    o.print2();
  }
}


// 5 6
// 11 0
